<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="mw_electronic_invoice.template_product_form" model="ir.ui.view">
            <field name="name">product.template.product.form.mw_inherited</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="arch" type="xml">
                 <xpath expr="//page[1]//group[1]//group[2]" position="inside">
                    <field name="mw_category"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>